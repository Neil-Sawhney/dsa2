./output/spell: ./src/spellcheck.o ./src/hash.o
	g++ -o ./output/spell ./src/spellcheck.o ./src/hash.o

./src/spellcheck.o: ./src/spellcheck.cpp ./include/hash.h
	g++ -c ./src/spellcheck.cpp

./src/hash.o: ./src/hash.cpp ./include/hash.h
	g++ -c ./src/hash.cpp

debug:
	g++ -g -o spellDebug spellcheck.cpp hash.cpp

clean:
	rm -f ./output/*
